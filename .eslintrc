{
  "env": {
    "commonjs": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "Uint8Array": false,
    "describe": false,
    "before": false,
    "it": false,
    "es6": true
  },
  "extends": "eslint:recommended",
  "parserOptions": { "ecmaVersion": 6 },
  "rules": {
    "no-console": "off",
    "semi": [
      "error",
      "always"
    ]
  }
}
